{
	"geode": "3.4.0",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206",
		"ios": "2.206"
	},
	"id": "alphalaneous.relocate_build_tools",
	"name": "Relocate Build Tools",
	"version": "v1.0.4",
	"developer": "Alphalaneous",
	"description": "Relocates the build tools that are in the editor pause menu to an editor tab",
	"dependencies": [
        {
            "id": "alphalaneous.editortab_api",
            "version": ">=v1.0.0-alpha.11",
            "importance": "required"
        },
		{
            "id": "geode.node-ids",
            "version": ">=v1.13.2",
            "importance": "required"
        }
    ],
	"links": {
        "homepage": "https://twitter.com/alphalaneous",
        "source": "https://github.com/Alphalaneous/Relocate-Build-Tools",
        "community": "https://discord.gg/Txn2CA5dHF"
    },
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"settings": {
		"icons": {
			"name": "Icons",
			"description": "Icons for the Build Tools Buttons",
			"type": "string",
			"default": "Icons & Text",
			"one-of": [
				"Icons & Text",
				"Icons Only",
				"None"
			]
		},
		"force-button-size": {
			"name": "Force Button Size",
			"description": "Forces the button size to stay the same no matter button row and column count",
			"type": "bool",
			"default": false
		}
	}
}
